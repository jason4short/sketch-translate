{
    "name" : "String Utility",
    "identifier" : "com.gopro.sketch.stringutility",
    "version" : "0.0.4",
    "description" : "Sketch plugin that exports and imports strings in a specific format for translation.",
    "author" : "GoPro Inc.",
    "commands" : [
    {
      "script" : "export.js",
      "handler" : "onRun",
      "shortcut" : "cmd ctrl e",
      "name" : "Export...",
      "identifier" : "export"
    },
    {
      "script" : "export.js",
      "handler" : "testPlugin",
      "shortcut" : "cmd ctrl p",
      "name" : "Export Options...",
      "identifier" : "export-options"
    },
    {
      "script" : "import.js",
      "handler" : "onRun",
      "shortcut" : "cmd ctrl i",
      "name" : "Import...",
      "identifier" : "import"
    }
  ]
}
